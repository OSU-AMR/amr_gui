   <string notr="true">/* Base widget style */
QWidget {
    background-color: #FAF3E0; /* Warm cream */
    color: #4E342E;           /* Deep brown */
    font-family: &quot;Cooper Black&quot;, &quot;Arial Rounded MT Bold&quot;, sans-serif;
    font-size: 14px;
}

/* QPushButton */
QPushButton {
    background-color: #FFB300; /* Mustard yellow */
    border: 2px solid #6D4C41;
    border-radius: 10px;
    padding: 8px;
    color: #4E342E;
}
QPushButton:hover {
    background-color: #FFA000;
}
QPushButton:pressed {
    background-color: #F57C00;
}

/* QLineEdit */
QLineEdit {
    background-color: #FFF8DC;
    border: 2px solid #8D6E63;
    border-radius: 6px;
    padding: 4px;
    color: #4E342E;
    selection-background-color: #FFD54F;
    selection-color: #4E342E;
}

/* QLabel */
QLabel {
    color: #4E342E;
}

/* QComboBox */
QComboBox {
    background-color: #FFF8DC;
    border: 2px solid #8D6E63;
    border-radius: 6px;
    padding: 4px;
    color: #4E342E;
}
QComboBox QAbstractItemView {
    background-color: #FFF3E0;
    border: 1px solid #8D6E63;
    selection-background-color: #FFD54F;
    selection-color: #4E342E;
}

/* QCheckBox and QRadioButton */
QCheckBox, QRadioButton {
    color: #4E342E;
}
QCheckBox::indicator, QRadioButton::indicator {
    border: 2px solid #8D6E63;
    background-color: #FFF8DC;
    width: 14px;
    height: 14px;
}
QCheckBox::indicator:checked, QRadioButton::indicator:checked {
    background-color: #FF8F00;
}

/* QTextEdit */
QTextEdit {
    background-color: #A9A9A9;
    color: #00FF00; /* Bright green text */
    font-family: &quot;Courier New&quot;, Courier, monospace;
    font-size: 10pt;
    border: none;
    padding: 8px;
    selection-background-color: #005500;
    selection-color: #00FF00;
}


/* QScrollBar */
QScrollBar:vertical, QScrollBar:horizontal {
    background: #FAF3E0;
    width: 10px;
}
QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background: #A1887F;
    border-radius: 5px;
}
QScrollBar::add-line, QScrollBar::sub-line {
    background: none;
}

/* QTabWidget */
QTabWidget::pane {
    border: 2px solid #6D4C41;
    background-color: #FAF3E0;
}
QTabBar::tab {
    background: #FFD54F;
    color: #4E342E;
    padding: 8px;
    border: 1px solid #8D6E63;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}
QTabBar::tab:selected {
    background: #FFB300;
}

/* QProgressBar */
QProgressBar {
    background-color: #FFF8DC;
    border: 2px solid #8D6E63;
    border-radius: 6px;
    color: #4E342E;
    text-align: center;
}
QProgressBar::chunk {
    background-color: #F57C00;
    width: 10px;
}
</string>
